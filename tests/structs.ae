/// out: "64\n64"

struct Square {
    x: i32
    y: i32
}

struct Shapes {
    s: Square
    b: &Square
}

def main() {
    let a: Square
    a.x = 5
    a.y = 10
    let z = &a
    z.x = 54
    print("%d\n", z.y + a.x)

    let sh: Shapes
    sh.s.x = 30
    sh.b = &a
    sh.b.y = 34
    print("%d\n", sh.s.x + sh.b.y)
}
